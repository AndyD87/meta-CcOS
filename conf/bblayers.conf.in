# POKY_BBLAYERS_CONF_VERSION is increased each time build/conf/bblayers.conf
# changes incompatibly
POKY_BBLAYERS_CONF_VERSION = "2"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  ${HOME}/poky-${YOCTO_BRANCH}/meta \
  ${HOME}/poky-${YOCTO_BRANCH}/meta-poky \
  ${HOME}/poky-${YOCTO_BRANCH}/meta-yocto-bsp \
  ${HOME} \
"
